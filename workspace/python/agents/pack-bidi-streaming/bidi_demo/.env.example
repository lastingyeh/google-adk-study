## 允許的 CORS 來源（以逗號分隔）
ALLOW_ORIGINS=http://localhost:8000

## 用於儲存日誌的 GCS bucket 名稱
LOGS_BUCKET_NAME=your-gcs-bucket-name

## 是否使用記憶體中的 session（測試用）
USE_IN_MEMORY_SESSION=true

## Agent Engine session 的名稱
AGENT_ENGINE_SESSION_NAME=pack-bidi-streaming

## agent 使用的模型名稱
DEMO_AGENT_MODEL=gemini-2.5-pro

## 是否使用 Vertex AI
GOOGLE_GENAI_USE_VERTEXAI=TRUE

## Gemini Live API 的 API 金鑰
GOOGLE_API_KEY=your_api_key_here

## Vertex AI 專案的 Google Cloud project ID
GOOGLE_CLOUD_PROJECT=your_project_id

## Vertex AI 的 Google Cloud 區域
GOOGLE_CLOUD_LOCATION=us-central1
