# 用於資源命名的專案名稱
project_name = "pack-auto-insurance-agent"

# 您的生產環境 (Production) Google Cloud 專案 ID
prod_project_id = "your-production-project-id"

# 您的測試環境 (Staging / Test) Google Cloud 專案 ID
staging_project_id = "your-staging-project-id"

# 用於裝載 Cloud Build 流線的 Google Cloud 專案 ID
cicd_runner_project_id = "your-cicd-project-id"

# 您在 Cloud Build 中建立的主機連接名稱
host_connection_name = "git-pack-auto-insurance-agent"

# GitHub PAT (個人存取權杖) 的 Secret ID
github_pat_secret_id = "your-github_pat_secret_id"

# GitHub 儲存庫擁有者（組織或使用者名稱）
repository_owner = "Your GitHub organization or username."

# 您添加到 Cloud Build 的儲存庫名稱
repository_name = "pack-auto-insurance-agent"

# 用於部署基礎設施的 Google Cloud 區域
region = "us-central1"
